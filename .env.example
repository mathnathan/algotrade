# Alpaca API Configuration 
# Set to false for live trading
PAPER=true  
# Alpaca paper trading account
APCA_PAPER_API_KEY_ID=
APCA_PAPER_API_SECRET_KEY=
APCA_PAPER_BASE_URL=https://paper-api.alpaca.markets/v2
# Alpaca live trading account
APCA_LIVE_API_KEY_ID=
APCA_LIVE_API_SECRET_KEY=
APCA_LIVE_BASE_URL=https://api.alpaca.markets/v2

# Huggingface Hub Configuration
HUGGINGFACE_HUB_TOKEN=
HUGGINGFACE_CACHE_DIR=/app/.cache/huggingface

# Database URLs
DATABASE_URL=postgresql://trading_user:secure_trading_password@postgres:5432/trading_db
POSTGRES_ADMIN_URL=postgresql://postgres:postgres_admin_password@postgres:5432/postgres